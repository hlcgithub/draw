        <mxCell id="P0RTmHD6Xz2b_fEOgWfL-7" value="环形数组从队列里获取任务" style="edgeLabel;html=1;align=center;verticalAlign=middle;resizable=0;points=[];" vertex="1" connectable="0" parent="P0RTmHD6Xz2b_fEOgWfL-6">
